prefix		=@prefix@
exec_prefix	=@exec_prefix@
bindir		=@bindir@
includedir	=@includedir@
libdir		=@libdir@
srcdir		=@srcdir@
VPATH		=@srcdir@

# compiler names:
CXX		=@CXX@
CC		=@CC@
MAKEDEPEND	=@MAKEDEPEND@

# flags for C++ compiler:
CFLAGS		=@CFLAGS@
CXXFLAGS	=@CXXFLAGS@ @X_CFLAGS@

# flags for the linker
LD_DLOPEN_FLAGS	=@LD_DLOPEN_FLAGS@	# for program that load some PLUGIN
LD_DLL_FLAGS	=@LD_DLL_FLAGS@		# to build some PLUGIN

# program to make the archive:
LIBNAME		=@LIBNAME@
LIBCOMMAND	=@LIBCOMMAND@
RANLIB		=@RANLIB@
DSOEXT		=@DSOEXT@
DSOLIBNAME	=@DSOLIBNAME@
DSOCOMMAND	=@DSOCOMMAND@

# libraries to link with:
LDLIBS		=@LDFLAGS@ @LIBS@ @LIBPNG@ @LIBJPEG@ @LDLIBS@ @X_EXTRA_LIBS@ -lm
LDDLLLIBS	=@LDFLAGS@ @LIBS@ @LIBPNG@ @LIBJPEG@ @LDDLLLIBS@ @X_EXTRA_LIBS@ -lm
GLDLIBS		=@LDFLAGS@ @LIBS@ @LIBPNG@ @LIBJPEG@ @GLLIB@ @LDLIBS@ @X_EXTRA_LIBS@ -lm

