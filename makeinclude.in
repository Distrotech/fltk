prefix		=@prefix@
exec_prefix	=@exec_prefix@
bindir		=@bindir@
includedir	=@includedir@
libdir		=@libdir@
srcdir		=@srcdir@
VPATH		=@srcdir@

SYSTYPE		=@SYSTYPE@

# compiler names:
CXX		=@CXX@
CC		=@CC@
MAKEDEPEND	=@MAKEDEPEND@

# flags for C++ compiler:
# WAS: I removed the -I@srcdir@, please use "xyz" rather than <xyz>
# for files that are in the same directory as the source file.
CFLAGS		=@DEBUGFLAG@ @CFLAGS@
CXXFLAGS	=@DEBUGFLAG@ @CXXFLAGS@
DSOCFLAGS	=@DSOCFLAGS@
DEBUGFLAGS	=@DEBUGFLAG@

# program to make the archive:
LIBNAME		=@LIBNAME@
LIBEXT		=@LIBEXT@
DSOLIBNAME	=@DSOLIBNAME@
DSOLIBformsNAME	=@DSOLIBformsNAME@
DSOLIBimagesNAME=@DSOLIBimagesNAME@
DSOLIBglNAME	=@DSOLIBglNAME@
DSOLIBglutNAME	=@DSOLIBglutNAME@
LIBCOMMAND	=@LIBCOMMAND@
RANLIB		=@RANLIB@
DSOEXT		=@DSOEXT@
DSOSHORTEXT	=@DSOSHORTEXT@
DSOCOMMAND	=@DSOCOMMAND@
THEMECOMMAND	=@THEMECOMMAND@

LD		=@LD@
EXEFLAGS	=@EXEFLAGS@

# libraries to link with:
LDLIBS		=@LDFLAGS@ @FLLIB@ @LIBS@ @LDLIBS@ @X_EXTRA_LIBS@ @LD_DLOPEN_FLAGS@
DSOLIBS		=@DSOLIBS@
THEMELIBS	=@THEMELIBS@
GLDLIBS		=@FLGLLIB@ @GLLIB@
GLUTLIBS	=@FLGLUTLIB@ @FLGLLIB@ @GLLIB@
FORMSLIBS	=@FLFORMSLIB@
IMAGELIBS	=@FLIMAGESLIB@
THREADSLIBS	=@THREADSLIBS@
JPEGLIBS	=@LIBJPEG@
PNGLIBS		=@LIBPNG@

FLLIB		=@FLLIB@
FLGLLIB		=@FLGLLIB@
FLGLUTLIB	=@FLGLUTLIB@
FLFORMSLIB	=@FLFORMSLIB@
FLIMAGESLIB	=@FLIMAGESLIB@

# Build commands and filename extensions...
.SUFFIXES:	.c .cxx .h .fl .o

