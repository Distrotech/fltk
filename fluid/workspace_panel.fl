Function {file_panel_set_cb(fltk3::Widget*, void *v)} {open return_type void
} {
  code {if (v == Fl_Panel::LOAD) {
  } else {
    fltk3::Widget*const* a = the_file_panel->array();
    for (int i=the_file_panel->children(); i--;) {
      fltk3::Widget* o = *a++;
      if (o->changed()) {
        o->do_callback();
        //if (haderror) return;
        o->clear_changed();
      }
    }
  }} {}
} 

