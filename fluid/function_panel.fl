Function {make_codeblock_panel()} {open
} {
  Fl_Window codeblock_panel {
    label codeblock
    xywh {539 470 290 131} resizable modal visible
  } {
    Fl_Input code_before_input {
      xywh {10 11 270 22} align 5 when NEVER
    }
    Fl_Box {} {
      label {"{...child code...}" is inserted here}
      xywh {10 33 270 33} align 20
    }
    Fl_Input code_after_input {
      xywh {10 65 270 23} align 5 when NEVER
    }
    Fl_Return_Button codeblock_panel_ok {
      label OK
      xywh {110 99 80 22} hotspot shortcut 0xff0d
    }
    Fl_Button codeblock_panel_cancel {
      label Cancel
      xywh {200 99 80 22} shortcut 0xff1b
    }
  }
} 

