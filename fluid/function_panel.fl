# data file for the FLTK User Interface Designer (FLUID)
version 2.0001 
images_dir ./ 
header_name {.h} 
code_name {.cxx} 
gridx 10 
gridy 11 
snap 3
Function {make_function_panel()} {open
} {
  Fl_Window function_panel {
    label {function/method}
    xywh {155 391 320 208} resizable modal visible
  } {
    Fl_Check_Button f_public_button {
      label public
      xywh {10 15 74 25} when NEVER
    }
    Fl_Check_Button f_c_button {
      label {C declaration}
      xywh {101 15 104 25}
    }
    Fl_Input f_name_input {
      label {Name(args): (blank for main())}
      xywh {10 55 300 22} align 5 when NEVER
    }
    Fl_Input f_attributes_input {
      label {Attributes (virtual, static, __declspec, ...)}
      xywh {10 99 300 22} align 5 when NEVER
    }
    Fl_Input f_return_type_input {
      label {Return Type: (blank to return outermost widget)}
      xywh {10 143 300 22} align 5 when NEVER
    }
    Fl_Return_Button f_panel_ok {
      label OK
      xywh {120 176 92 22} hotspot shortcut 0xff0d
    }
    Fl_Button f_panel_cancel {
      label Cancel
      xywh {220 176 90 22} shortcut 0xff1b
    }
  }
} 

